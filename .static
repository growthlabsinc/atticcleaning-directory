{
  "headers": {
    "/*.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/*.json": {
      "Cache-Control": "public, max-age=3600"
    },
    "/*.html": {
      "Cache-Control": "public, max-age=300, must-revalidate"
    },
    "/*.webp": {
      "Cache-Control": "public, max-age=2592000"
    },
    "/*.png": {
      "Cache-Control": "public, max-age=2592000"
    },
    "/*.jpg": {
      "Cache-Control": "public, max-age=2592000"
    },
    "/*.jpeg": {
      "Cache-Control": "public, max-age=2592000"
    }
  },
  "redirects": {
    "/index.html": {
      "to": "/",
      "status": 301
    }
  },
  "error_pages": {
    "404": "/404.html"
  }
}
